#: ../source/modules.rst:22
msgid "utils.file_utils"
msgstr "utils.file_utils"

#: of taggy.utils.file_utils:1
msgid ""
"Utility functions for file operations, creating shortcuts (Windows), and "
"saving metadata."
msgstr ""
"Funkcje narzędziowe do operacji na plikach, tworzenia skrótów (Windows) i "
"zapisywania metadanych."

#: of taggy.utils.file_utils.copy_file:1
msgid ""
"Copies a file from src to dest, preserving metadata (timestamps, etc.)."
msgstr ""
"Kopiuje plik z src do dest, zachowując metadane (znaczniki czasu itp.)."

#: of taggy.utils.file_utils.copy_file:3
#: taggy.utils.file_utils.perform_file_operation:3
msgid "Source file path."
msgstr "Ścieżka pliku źródłowego."

#: of taggy.utils.file_utils.copy_file:5
msgid "Destination file path."
msgstr "Ścieżka pliku docelowego."

#: of taggy.utils.file_utils.create_directory:1
msgid "Creates a directory if it does not exist."
msgstr "Tworzy katalog, jeśli nie istnieje."

#: of taggy.utils.file_utils.create_directory:3
msgid "Path to the directory."
msgstr "Ścieżka do katalogu."

#: of taggy.utils.file_utils.create_shortcut:1
msgid "Creates a Windows shortcut (.lnk) to the target file."
msgstr "Tworzy skrót Windows (.lnk) do pliku docelowego."

#: of taggy.utils.file_utils.create_shortcut:3
msgid "Absolute path to the target file."
msgstr "Ścieżka bezwzględna do pliku docelowego."

#: of taggy.utils.file_utils.create_shortcut:5
msgid "Absolute path where the shortcut will be created."
msgstr "Ścieżka bezwzględna, w której zostanie utworzony skrót."

#: of taggy.utils.file_utils.create_shortcut:7
msgid "Shortcut description. Defaults to None."
msgstr "Opis skrótu. Domyślnie Brak."

#: of taggy.utils.file_utils.create_shortcut:9
msgid "Path to an icon file. Defaults to None."
msgstr "Ścieżka do pliku ikony. Domyślnie Brak."

#: of taggy.utils.file_utils.list_supported_image_files:1
msgid ""
"Returns a list of supported image file paths in the given directory. "
"Supported extensions: png, jpg, jpeg, bmp, webp"
msgstr ""
"Zwraca listę obsługiwanych ścieżek plików graficznych w podanym katalogu. "
"Obsługiwane rozszerzenia: png, jpg, jpeg, bmp, webp"

#: of taggy.utils.file_utils.list_supported_image_files:4
msgid "Path to a folder containing images."
msgstr "Ścieżka do folderu zawierającego obrazy."

#: of taggy.utils.file_utils.list_supported_image_files:7
msgid "List of file paths found with supported extensions."
msgstr "Lista znalezionych ścieżek plików z obsługiwanymi rozszerzeniami."

#: of taggy.utils.file_utils.load_config:1
msgid ""
"Loads default values from an INI config file and returns them as a "
"dictionary-like object."
msgstr ""
"Wczytuje wartości domyślne z pliku konfiguracyjnego INI i zwraca je jako "
"obiekt podobny do słownika."

#: of taggy.utils.file_utils.load_config:3
msgid "The path to the config file. Defaults to \"config.ini\"."
msgstr "Ścieżka do pliku konfiguracyjnego. Domyślnie \"config.ini\"."

#: of taggy.utils.file_utils.load_config:6
msgid "A config section (dict-like) with default values."
msgstr "Sekcja konfiguracji (podobna do dict) z wartościami domyślnymi."

#: of taggy.utils.file_utils.perform_file_operation:1
msgid ""
"Performs a file operation (copy or symlink/shortcut) from src to dest_dir."
msgstr ""
"Wykonuje operację na pliku (kopiowanie lub dowiązanie symboliczne/skrót) z "
"src do dest_dir."

#: of taggy.utils.file_utils.perform_file_operation:5
msgid "Destination directory path."
msgstr "Ścieżka katalogu docelowego."

#: of taggy.utils.file_utils.perform_file_operation:7
msgid "Type of operation ('copy', 'symlink', etc.)."
msgstr "Typ operacji (\"kopia\", \"dowiązanie symboliczne\" itp.)."

#: of taggy.utils.file_utils.perform_file_operation:9
#: taggy.utils.file_utils.perform_file_operation:11
msgid "Used if creating a shortcut. Defaults to None."
msgstr "Używane podczas tworzenia skrótu. Domyślnie ustawiona na Brak."

#: of taggy.utils.file_utils.preprocess_image:1
msgid ""
"Opens and preprocesses a single image using the provided CLIP preprocess "
"pipeline."
msgstr ""
"Otwiera i wstępnie przetwarza pojedynczy obraz przy użyciu dostarczonego "
"potoku wstępnego przetwarzania CLIP."

#: of taggy.utils.file_utils.preprocess_image:5
msgid "The CLIP preprocess transform."
msgstr "Przekształcenie wstępne CLIP."

#: of taggy.utils.file_utils.preprocess_image:7
msgid "'cpu' or 'cuda'."
msgstr "'cpu' lub 'cuda'."

#: of taggy.utils.file_utils.preprocess_image:10
msgid "The preprocessed image on the appropriate device.z"
msgstr "Wstępnie przetworzony obraz na odpowiednim urządzeniu.z"

#: of taggy.utils.file_utils.save_metadata_to_json:1
msgid "Saves metadata to a JSON file."
msgstr "Zapisuje metadane do pliku JSON."

#: of taggy.utils.file_utils.save_metadata_to_json:3
msgid "Metadata to be saved."
msgstr "Metadane do zapisania."

#: of taggy.utils.file_utils.save_metadata_to_json:5
msgid "Path to the output JSON file."
msgstr "Ścieżka do wyjściowego pliku JSON."

